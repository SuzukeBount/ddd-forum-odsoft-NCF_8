{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["./dist"],
      "includePattern": "\\.(jsx|js|ts|tsx)$",
      "exclude": ["node_modules", "src/**/*.spec.ts", "src/**/*.spec.js"],
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "better-docs": {
        "name": "Your App Name"
      }
    },
    "opts": {
      "destination": "./docs/jsdoc",
      "recurse": true,
      "readme": "README.md"
    }
  }